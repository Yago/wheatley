    "axios": "^0.26.1",
    "cheerio": "^1.0.0-rc.10",
    "concurrently": "^7.6.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.0",
    "firebase": "^9.6.11",
    "nanoid": "^3.3.3",
    "nodemon": "^2.0.15",
    "ramda": "^0.29.0"
  }
}
